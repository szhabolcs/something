{
  "type": "module",
  "scripts": {
    "generate": "drizzle-kit generate:pg",
    "dev": "tsx watch src/index.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/index.js",
    "lint": "eslint --fix . && npm run typecheck",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:check": "drizzle-kit check --config ./src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./src/db/drizzle.config.ts",
    "update-packages": "npm-check-updates -i --format group -x eslint"
  },
  "engines": {
    "node": "^20.10.0"
  },
  "dependencies": {
    "@hono/node-server": "1.11.2",
    "@hono/swagger-ui": "0.2.2",
    "@hono/zod-openapi": "0.14.2",
    "@scalar/hono-api-reference": "0.5.62",
    "bcrypt": "5.1.1",
    "dotenv": "16.4.5",
    "drizzle-orm": "0.31.2",
    "hono": "4.4.4",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "npm-check-updates": "16.14.20",
    "pg": "8.12.0",
    "postgres": "3.4.4",
    "rimraf": "5.0.7",
    "sharp": "0.33.4",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcrypt": "5.0.2",
    "@types/node": "20.14.2",
    "@types/pg": "8.11.6",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "drizzle-kit": "0.22.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.3.1",
    "tsx": "4.13.2",
    "typescript": "5.4.5"
  },
  "private": true
}
